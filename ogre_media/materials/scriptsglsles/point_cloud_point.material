material rviz/PointCloudPoint
{
  technique gp
  {
    pass
    {
      alpha_rejection greater_equal 1
      point_size_attenuation on
      point_sprites on
      vertex_program_ref   rviz/glsles/point.vert {}
      fragment_program_ref rviz/glsles/flat_color_circle.frag {}
    }
  }
  
  technique depth
  {
    scheme Depth
    pass
    {
      point_size_attenuation on
      vertex_program_ref rviz/glsles/point.vert(with_depth) {}
      fragment_program_ref rviz/glsles/depth_circle.frag {}
    }
  }
  
  technique selection_first_pass
  {
    scheme Pick
    pass
    {
      point_size_attenuation on
      vertex_program_ref rviz/glsles/point.vert {}
      fragment_program_ref rviz/glsles/pickcolor_circle.frag {}
    }
  }
  
  technique selection_second_pass
  {
    scheme Pick1
    pass
    {
      point_size_attenuation on
      vertex_program_ref rviz/glsles/point.vert {}
      fragment_program_ref rviz/glsles/pass_color_circle.frag {}
    }
  }
}
