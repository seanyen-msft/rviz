material rviz/PointCloudSphere {
  
  technique nogp {
    pass { 
      alpha_rejection greater_equal 1
      vertex_program_ref   rviz/glsles/nogp/billboard.vert {}
      fragment_program_ref rviz/glsles/shaded_circle.frag {}
    }
  }
  
  technique nogp_depth { 
    scheme Depth
    pass { 
      alpha_rejection greater_equal 1
      vertex_program_ref   rviz/glsles/nogp/billboard.vert(with_depth) {}
      fragment_program_ref rviz/glsles/depth_circle.frag {}
    }
  }
  
  technique nogp_selection_first_pass { 
    scheme Pick
    pass {
      alpha_rejection greater_equal 1
      vertex_program_ref   rviz/glsles/nogp/billboard.vert {}
      fragment_program_ref rviz/glsles/pickcolor_circle.frag {}
    }
  }
  
  technique nogp_selection_second_pass {
    scheme Pick1
    pass { 
      alpha_rejection greater_equal 1
      vertex_program_ref   rviz/glsles/nogp/billboard.vert {}
      fragment_program_ref rviz/glsles/pass_color_circle.frag {}
    }
  }
  
}
